// 按钮样式
.solid-with-icon-btn {
  font-size: 14px !important;
  border-radius: 4px !important;
  background: linear-gradient(1turn,#4576e4,#6f9ef0) !important;
  color: #fff !important;
  padding: 7px 15px;
}

.hollow-with-icon-btn {
  font-size: 14px !important;
  color: #4676e5 !important;
  background: #fff !important;
  border-color: #419fff !important;
  padding: 7px 15px;
}
