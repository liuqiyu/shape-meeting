import ButtonList from './advanced/asp-button-list'
import UploadFile from './advanced/asp-upload-file'
// import SmartDialog from './advanced/smart-dialog'
// 基础组件
import Elements from './element'
console.log(Elements)

const AdvancedComponents = [ButtonList, UploadFile] // 高阶组件

const AllComponents = [...Elements, ...AdvancedComponents] // 所有组件集合

const install = (Vue, opts = {}) => {
  AllComponents.filter(v => typeof v !== 'function').forEach(component => {
    Vue.component(component.name, component)
  })

  Vue.prototype.$ASPWEBLIB = {
    style: opts.style || ''
  }
}

/* istanbul ignore if */
if (typeof window !== 'undefined' && window.Vue) {
  install(window.Vue)
}

export default {
  version: '1.0.2',
  install,
  ButtonList,
  UploadFile,
  Elements
}